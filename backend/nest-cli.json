{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "watchAssets": true,
    "assets": []
  },
  "watchOptions": {
    "usePolling": false,
    "interval": 1000,
    "binaryInterval": 3000
  }
}

